{
  "name": "critickeroverhaul",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "preBuild": "npm run delSymlinks && npm run createSymlinks",
    "--": "",
    "clientBuild": "cd ./client && webpack --mode production",
    "clientSync": "cd ./client && aws s3 rm s3://react-bucket-x3blze4m-dev --recursive && aws s3 sync ./dist s3://react-bucket-x3blze4m-dev",
    "clientDeploy": "npm run clientBuild && npm run clientSync",
    "slsDeploy": "cd ./backend && serverless deploy --verbose",
    "---": "",
    "clean": "cd ./backend && RMDIR /Q/S .serverless 2>nul && cd ./client && RMDIR /Q/S dist 2>nul",
    "-v": "",
    "fullDeploy": "npm run preBuild && npm run clientDeploy && npm run slsDeploy",
    "v": "",
    "deployClientLocally": "cd ./client && npx webpack serve --mode=development"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "csvtojson": "^2.0.10",
    "depcheck": "^1.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-check-updates": "^12.0.2",
    "prettier": "^2.4.1",
    "remove-node-modules": "^1.7.6"
  },
  "devDependencies": {
    "@types/csvtojson": "^2.0.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonathanMSifleet/CritickerOverhaul.git"
  },
