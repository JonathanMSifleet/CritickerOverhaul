{
  "name": "critickeroverhaul",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "buildClient": "cd ./client && webpack --mode production",
    "syncClient": "cd ./client && aws s3 rm s3://react-bucket-x3blze4m-dev --recursive && aws s3 sync ./dist s3://react-bucket-x3blze4m-dev",
    "deployClient": "npm run clientBuild && npm run clientSync",
    "slsDeploy": "cd ./backend && serverless deploy --verbose",
    "-": "",
    "clean": "cd ./backend && RMDIR /Q/S .serverless 2>nul && cd ./client && RMDIR /Q/S dist 2>nul",
    "--": "",
    "fullDeploy": "npm run preBuild && npm run clientDeploy && npm run slsDeploy",
    "---": "",
    "serve": "cd ./client && webpack serve --mode=development",
    "forceUpdate": "npx npm-check-updates -u && npm i"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "compress.js": "^1.2.2",
