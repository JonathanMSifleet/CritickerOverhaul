{
  "name": "finalyearproject",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "createSymlinks": "mklink .\\backend\\package.json ..\\package.json & mklink .\\backend\\package-lock.json ..\\package-lock.json & mklink /D .\\backend\\node_modules ..\\node_modules & mklink .\\client\\package.json ..\\package.json & mklink .\\client\\package-lock.json ..\\package-lock.json & mklink /D .\\client\\node_modules ..\\node_modules",
    "delSymlinks": "del .\\backend\\package.json & del .\\backend\\package-lock.json & del .\\client\\package.json & del .\\client\\package-lock.json & RMDIR /Q/S backend\\node_modules 2>nul & RMDIR /Q/S client\\node_modules 2>nul",
    "updateSymlinks": "npm run delSymlinks && npm run createSymlinks",
    "preBuild": "npm run updateSymlinks",
    "postBuild": "cd ./backend && RMDIR /Q/S .serverless 2>nul",
    "slsDeploy": "cd ./backend && serverless deploy -v",
    "-": "",
    "fullDeploy": "npm run preBuild && npm run slsDeploy && npm run postBuild",
    "--": "",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "---": "",
    "removeNodeModules": "npx remove-node-modules"
  },
  "comments": {
    "fullDeploy": "run ng build, deploy serverless files, remove existing files from s3 bucket, upload ng build result to S3 bucket, delete uncessary build and serverless files"
  },
